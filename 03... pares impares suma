#include <iostream>
using namespace std;
int main() {
    int n;
    cin>>n;
    int vector[n];
    int SP=0;
    int SI=0;
    
    for (int i=0 ; i<n ; i++){
        vector[i]=i+1;
         if(vector[i]%2==0){
            SP=SP+vector[i];
        }else {
            SI=SI+vector[i];
        }
    }
    cout<<"SUMA DE PARES:"<<SP<<endl;
    cout<<"SUMA DE IMPARES:"<<SI;
}
